{
  "bracketSpacing": true,
  "bracketSameLine": true,
  "singleQuote": false,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "useTabs": false,
  "tabWith": 2,
  "printWidth": 120,
  "importOrder": ["^react", "^next|next/(.*)$", "<THIRD_PARTY_MODULES>", "^components/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "endOfLine": "auto"
}
